{
    "name": "beelab/tag-bundle",
    "type": "symfony-bundle",
    "description": "Provide tags for Doctrine entities in Symfony",
    "keywords": [
        "Tag",
        "Tags",
        "Symfony"
    ],
    "homepage": "https://github.com/bee-lab/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/orm": "^2.6",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/routing": "^3.4 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Beelab\\TagBundle\\": ""
        }
    }
}
