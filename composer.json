{
    "name": "beelab/tag-bundle",
    "type": "symfony-bundle",
    "description": "Provide tags for Doctrine entities in Symfony",
    "keywords": [
        "Tag",
        "Tags",
        "Symfony"
    ],
    "homepage": "https://github.com/bee-lab/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "garakkio@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/orm": "^2.7",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/form": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/routing": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Beelab\\TagBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beelab\\TagBundle\\Tests\\": "tests"
        }
    }
}
